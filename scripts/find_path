import json
json_path = "scripts/report_sq.json"

with open(json_path, "r", encoding="utf-8") as f:
    report_data = json.load(f)
    py_list = report_data.get("py", [])
    yaml_list = report_data.get("yaml", [])

    for py in py_list:
        path = py.get("path", "")
        comments = py.get("comments", [])
        docstrings = py.get("docstrings", [])
        if comments or docstrings:
            print(f"\n[PY] {path}")
    for yaml in yaml_list:
        path = yaml.get("path", "")
        matches = yaml.get("matches", [])
        if matches:
            print(f"\n[YAML] {path}")
print(f"total file num: {len(py_list) + len(yaml_list)}")